#include <stdio.h>

int main(void) {
    int n;
    int x,y;
    scanf("%d",&n);
    for (int i=0;i<n;i++){
        scanf("%d %d",&x, &y);
        if ((x*2)>(y*5)){
            printf("\nChocolate");
        }else if ((y*5)>(x*2)){
            printf("\nCandy");
        }else {
            printf("\nEither");
        }
    }
	// your code goes here
	return 0;
}

