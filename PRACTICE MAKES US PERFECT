#include <stdio.h>

int main(void) {
    int N,M,K;
    int c;
    
    scanf("%d",&c);
    for(int i=0;i<c;i++){
        scanf("%d",&N);
        scanf("%d",&M);
        scanf("%d",&K);
    
    if (M-K>N-1){
        printf("Yes\n");
    }else {
        printf("No\n");
    }
    }
	// your code goes here
	return 0;
}
